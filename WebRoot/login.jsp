	<body class="login-layout">
		<form action="login" method="post">
		<div class="main-container">
			<div class="main-content">
				<div class="row">
					<div class="col-sm-10 col-sm-offset-1">
						<div class="login-container">
							<div class="center">
							<br>
								<h3>
									<i class="icon-leaf green"></i>
									<span class=red>江苏</span>
									<span class="black">电网输电铁塔变形监测系统</span>
								</h3>
								<br>
								<h4 class="blue">&copy; 华能智创</h4>
							</div>

							<div class="space-6"></div>

							<div class="position-relative">
								<div id="login-box" class="login-box visible widget-box no-border">
									<div class="widget-body">
										<div class="widget-main">
											<h4 class="header blue lighter bigger">
												<i class="icon-coffee green"></i>
												欢迎访问
											</h4>

											<div class="space-6"></div>

											<form>
												<fieldset>
													<label class="block clearfix">
														<span class="block input-icon input-icon-right">
															<input type="text" name="password" class="form-control" placeholder="用户名" />
															<i class="icon-user"></i>
														</span>
													</label>

													<label class="block clearfix">
														<span class="block input-icon input-icon-right">
															<input type="password" name="userName" class="form-control" placeholder="密码" />
															<i class="icon-lock"></i>
														</span>
													</label>

													<div class="space"></div>

													<div class="clearfix">
														<label class="inline">
															<!-- <input type="checkbox" class="ace" />
															<span class="lbl"> 记住我</span> -->
														</label>

														<button type="submit" class="width-35 pull-right btn btn-sm btn-primary">
															<i class="icon-key"></i>
															登陆
														</button>
													</div>

													<div class="space-4"></div>
												</fieldset>
											</form>

											<div class="social-or-login center">
												<span class="bigger-110">适用于</span>
											</div>

											<div class="social-login center">
												<a class="btn btn-primary">
												 江
												</a>

												<a class="btn btn-info">
												苏
												</a>

												<a class="btn btn-danger">
												电
												</a>
												<a class="btn btn-warn">
												网
												</a>
											</div>
										</div><!-- /widget-main -->
									</div><!-- /widget-body -->
								</div><!-- /login-box -->


							</div><!-- /position-relative -->
						</div>
					</div><!-- /.col -->
				</div><!-- /.row -->
			</div>
		</div><!-- /.main-container -->
		<input type="hidden" id="messageFlag" value="${messageFlag }" />
