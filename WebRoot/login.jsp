		<script type="text/javascript">
			messageShow();
			function show_box(id) {
			 jQuery('.widget-box.visible').removeClass('visible');
			 jQuery('#'+id).addClass('visible');
			}
			function messageShow() {
				var flag = $("#messageFlag").val();
				if(flag == 1){
					alert("账号或者密码错误，请重新输入！");
				}
				}
